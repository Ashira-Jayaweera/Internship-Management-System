/* Creates database "MSDDB" */
--CREATE DATABASE MSDDB;
/* Switch to "MSDDB" */
--USE MSDDB;
/* Create database */
CREATE TABLE IF NOT EXISTS password_table (id int(5) NOT NULL AUTO_INCREMENT, username varchar(50) NOT NULL, password varchar(300) NOT NULL, user_type BOOL NOT NULL, PRIMARY KEY(id));
CREATE TABLE IF NOT EXISTS admin_table (id int(5) NOT NULL AUTO_INCREMENT, admin_name varchar(50) NOT NULL, password varchar(200) not null, PRIMARY KEY(id));
/* Check table */
SHOW columns IN password_table;